{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": [
		"airbnb-base"
	],
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"rules": {
		"no-underscore-dangle": "off",
		"newline-per-chained-call": "off",
		"class-methods-use-this": "off",
		"no-param-reassign": "off",
		"no-plusplus": "off"
	},
	"settings": {
		"import/extensions": [
			".js"
		]
	}
}
